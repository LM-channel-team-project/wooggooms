<div id="groupListContainer">
  <% data.forEach(function(group) { %>
  <table onclick="location.href=''">
    <tr>
      <th rowspan="3" class="thumnailContainer">
        <img class="thumnail" src="<%= group.thumnail %>" />
      </th>
      <th colspan="2" class="groupNameContainer"><%= group.name %></th>
    </tr>
    <tr>
      <td class="groupLocationContainer"><%= group.location %></td>
      <td class="groupNumContainer">
        모집중 <%= group.num_member %>/<%= group.max_num_member %>
      </td>
    </tr>
    <tr>
      <td colspan="2" class="groupTagContainer"><%= group.tag %></td>
    </tr>
  </table>
  <% }); %>
</div>
